package helper;

public class Prime {

	private static boolean isaPrime(long nbr) {
		for(long j = nbr -1; j > 1; j--) {
			if(nbr%j == 0) {
				return false;
			}
		}
		
		return true;
	}
	public static void main(String[] args) {
		long nbr = 600851475143L;
	
		long factor = 0;
		for(long i = 5; i < nbr; i++) {
			if(isaPrime(i)) {
				if(nbr%i == 0) {
					factor = i;
				}
			}
		}
		
		System.out.print(factor);
		
		
	}
}
