package helper;

public class SmallestDiviser {

	
	public static void main(String[] args) {
		for(int i = 2520; i < Integer.MAX_VALUE; i++) {
			if(i%1 == 0) 
				if(i%2 == 0)
					if(i%3 == 0)
						if(i%4 == 0)
							if(i%5 == 0)
								if(i%6 == 0)
									if(i%7 == 0)
										if(i%8 == 0)
											if(i%9 == 0)
												if(i%10 == 0) {
													System.out.println(i);
													break;
												}
													
			
		}
	}
}
