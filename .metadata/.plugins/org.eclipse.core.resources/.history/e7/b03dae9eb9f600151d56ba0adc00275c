package helper;

public class Prime {

	private static boolean isaPrime(long nbr) {
		for(long i = nbr -1; i > 1; i--) {
			if(nbr%i == 0) {
				return false;
			}
		}
		
		return true;
	}
	public static void main(String[] args) {
//		long nbr = 600851475143L;
		int nbr = 13195;
		long factor = 0;
//		System.out.print(isaPrime(114));
		long i = nbr/2;
		while(factor == 0 ) {
			if(isaPrime(i)) {
				if(nbr%i == 0) {
					factor = i;
				}
			}
			
			i--;
		}
		System.out.print(factor);
		
		
	}
}
